<div class="page" id="page">
	{% include 'organisms-header' %}
	<div role="main">
		<h1 class="section-title">Our Outdoor Blog</h1>
		<div class="l-two-col">
			<div class="l-main">
				<section class="section latest-posts">

					<ul class="post-list">
						{% if latest-posts %}
							{% include 'molecules-media-block' %}
						{% endif %}
					</ul>
					<a href="#" class="text-btn">View more posts</a>
				</section>
				{% include 'molecules-pagination' %}
			</div><!--end l-main-->

			<div class="l-sidebar">
				{% include 'organisms-recent-tweets' %}
			</div><!--end l-sidebar-->
		</div><!--end two-col-->
	</div><!--End role=main-->
	{% include 'organisms-footer' %}
</div>
